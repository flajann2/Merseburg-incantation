# - Config file for the Merseberg package
# It defines the following variables
#  MERSEBERG_INCLUDE_DIRS - include directories for Merseberg
#  MERSEBERG_LIBRARIES    - libraries to link against
#  MERSEBERG_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(MERSEBERG_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(MERSEBERG_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET foo AND NOT Merseberg_BINARY_DIR)
  include("${MERSEBERG_CMAKE_DIR}/MersebergTargets.cmake")
endif()

# These are IMPORTED targets created by MersebergTargets.cmake
set(MERSEBERG_LIBRARIES merseberg)

